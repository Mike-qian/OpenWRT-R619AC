# 2. AES算法支持（硬件/软件双兼容）
CONFIG_CRYPTO_AES=y
CONFIG_CRYPTO_AES_ARM=y  # ARM软解AES（兜底，硬件加速优先）
CONFIG_CRYPTO_AES_CTR=y
CONFIG_CRYPTO_AES_CBC=y
CONFIG_CRYPTO_AES_GCM=y  # 常用的AES-GCM（WPA3/IPSec）
CONFIG_CRYPTO_AES_NI=n   # 禁用x86 AES-NI（无关）

# 3. 高通QCA加密引擎驱动（核心：启用IPQ4019硬件AES加速）
CONFIG_CRYPTO_DEV_QCA=y
CONFIG_CRYPTO_DEV_QCA_COMMON=y
CONFIG_CRYPTO_DEV_QCA_CRYPTO=y
CONFIG_CRYPTO_DEV_QCA_AES=y  # 显式启用AES硬件加速
CONFIG_CRYPTO_DEV_QCA_SHA=y  # 顺带开启SHA硬件加速（配套AES使用）

# 4. 加密算法卸载到硬件（确保内核优先调用硬件引擎）
CONFIG_CRYPTO_HW=y
CONFIG_CRYPTO_DEV=y
CONFIG_CRYPTO_USER_API=y
CONFIG_CRYPTO_USER_API_AEAD=y  # 支持AES-GCM等AEAD模式
CONFIG_CRYPTO_USER_API_HASH=y
# 1. OpenSSL（最常用的加密库，启用硬件引擎适配）
CONFIG_PACKAGE_libopenssl=y
#CONFIG_PACKAGE_libopenssl-dev=y  # 开发库（可选，编译自定义程序用）
CONFIG_OPENSSL_WITH_ASM=y       # 启用ARM汇编优化（软解兜底）
CONFIG_OPENSSL_WITH_HW_CRYPTO=y # 启用硬件加密引擎适配
CONFIG_OPENSSL_WITH_AES=y
CONFIG_OPENSSL_WITH_GCM=y

# 2. IPSec/OpenVPN（常用加密场景，依赖AES加速）
CONFIG_PACKAGE_openvpn-openssl=y  # OpenVPN启用OpenSSL（自动调用硬件AES）
CONFIG_PACKAGE_strongswan=y        # IPSec（StrongSwan）适配硬件加密
CONFIG_STRONGSWAN_WITH_AES=y
CONFIG_STRONGSWAN_WITH_GCM=y

# 3. WPA3（依赖AES-GCM，必须开启）
CONFIG_PACKAGE_hostapd-common=y
CONFIG_HOSTAPD_WPA3=y
CONFIG_HOSTAPD_CRYPTO=y

# 启用AF_ALG套接字（用户态调用内核加密引擎的核心接口）
CONFIG_CRYPTO_USER_API=y
CONFIG_CRYPTO_USER_API_ALG=y
CONFIG_CRYPTO_USER_API_AEAD=y  # 支持AES-GCM（TLS 1.2/1.3主流模式）
CONFIG_CRYPTO_USER_API_HASH=y  # 支持SHA-256（TLS哈希校验）
CONFIG_CRYPTO_USER_API_SKCIPHER=y  # 支持对称加密（AES-CBC/CTR）

# 确保QCA驱动绑定af_alg
CONFIG_CRYPTO_DEV_QCA=y
CONFIG_CRYPTO_DEV_QCA_AES=y
CONFIG_CRYPTO_DEV_QCA_SHA=y
CONFIG_CRYPTO_DEV_QCA_ECC=y  # TLS 1.3 ECDHE密钥交换加速

# OpenSSL启用硬件加密引擎
CONFIG_PACKAGE_libopenssl=y
CONFIG_OPENSSL_WITH_HW_CRYPTO=y  # 关键：启用硬件加密适配
CONFIG_OPENSSL_WITH_ASM=y        # ARM NEON汇编优化（软解兜底）
CONFIG_OPENSSL_WITH_AES=y
CONFIG_OPENSSL_WITH_GCM=y
CONFIG_OPENSSL_WITH_SHA256=y
CONFIG_OPENSSL_WITH_EC=y         # TLS 1.3 ECC密钥交换

# 可选：启用OpenSSL引擎框架（手动指定硬件引擎）
CONFIG_OPENSSL_ENGINES=y
CONFIG_OPENSSL_ENGINE_CRYPTODEV=y  # 绑定内核crypto设备
